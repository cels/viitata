{
  "name": "__MSG_name__",
  "short_name": "__MSG_shortName__",
  "description": "__MSG_description__",
  "version": "1.0",
  "author": "bla",
  "homepage_url": "https://github.com/cels/viitata",
  "icons": {
    "128": "images/icon-128.png",
